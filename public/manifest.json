{
  "manifest_version": 2,
  "name": "Boilerplate extension",
  "author": "Trung Nguyen - Kenny",
  "version": "0.0.1",
  "description": "Replace new tab screen with GitHub trending projects.",
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage", "alarms"]
}